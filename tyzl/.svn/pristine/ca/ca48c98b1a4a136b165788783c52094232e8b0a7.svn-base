<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
	            SELECT 	
				    QT.TRIGGER_NAME, 
					QT.TRIGGER_GROUP, 
					QT.JOB_NAME, 
					QT.JOB_GROUP, 
					QT.IS_VOLATILE, 
					QT.DESCRIPTION,
					(CASE WHEN QJD.DESCRIPTION IS NULL THEN QJD.JOB_NAME ELSE QJD.DESCRIPTION END) AS JOB_DESCRIPTION , 
					QT.NEXT_FIRE_TIME, 
					QT.PREV_FIRE_TIME, 
					QT.PRIORITY, 
					QT.TRIGGER_STATE, 
					QT.TRIGGER_TYPE, 
					QT.START_TIME, 
					QT.END_TIME, 
					QT.CALENDAR_NAME, 
					QT.MISFIRE_INSTR, 
					QT.JOB_DATA ,
					QCT.CRON_EXPRESSION
					FROM QRTZ_TRIGGERS  QT
					LEFT JOIN QRTZ_JOB_DETAILS QJD ON QJD.JOB_NAME = QT.JOB_NAME
					LEFT JOIN QRTZ_CRON_TRIGGERS QCT ON QCT.TRIGGER_NAME = QT.TRIGGER_NAME
			 ]]> 
	  </table_sql>
	  </alldb>
      <dataSource>dataSource</dataSource>
    </data>
  </table>
</table-root>