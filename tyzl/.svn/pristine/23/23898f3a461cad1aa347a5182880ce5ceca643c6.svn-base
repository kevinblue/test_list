<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
				SELECT 
				t.ID               as id,
				t.OWN_NAME         as ownname,
				t.OWN_NUMBER	   as ownnumber,
				t.LEASECONSIGNER   as leaseconsigner,
				t.LEASE_REGISTER_ADDR  as leaseregisteraddr,
				t.LEASE_ADDR       as leaseaddr,
				t.LEASE_PERSON     as leaseperson,
				t.LEASE_POSTCODE   as leasepostcode,
				t.LEASE_LINKMAN    as leaselinkman,
				t.LEASE_TEL        as leasetel,
				t.LEASE_FAX        as leasefax,
				t.LEASE_EMAIL      as leaseemail
				FROM own_info t 
				where 1=1
				and t.id  = '{id}'
			 ]]> 
	  </table_sql>
	  </alldb>
      <dataSource>dataSource</dataSource>
      <show_sql>true</show_sql>
    </data>
  </table>
</table-root>