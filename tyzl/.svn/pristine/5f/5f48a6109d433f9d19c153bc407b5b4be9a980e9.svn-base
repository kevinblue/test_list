html,body {
	padding: 0 0 0 0;
	margin: 0 0 0 0;
	overflow: hidden;
}

.main_topbg {
	text-align: center;
	width: 100%;
	background: #263849 url(../../images/topbg.png) repeat-x;
	border-bottom: 1px solid #16222C;
	color: #DDD;
	float:left;
}

.main_logo {
	position: relative;
	float: left;
	width: 150px;
	height: 100%;
	top: 12px;
	left: 20px;
	background: url(../../images/TLSv45.gif) no-repeat;
}

.main_logo a {
	float: left;
	padding-top: 3px;
	padding-left: 75px;
	font: bold 30px/120% "黑体";
	width: 500px;
}

.main_logo a  font.font_cls {
	font-size: 20px
}

.main_logo_img {
	position: relative;
	float: left;
	height: 100%;
}

.main_time {
	position: absolute;
	right: 20px;
	font-size: 12px;
	line-height: 200%;
	margin-top: 8px;
	text-align: right;
}

.main_time a {
	color: #FFF;
	text-decoration: none;
}

.main_time a:hover {
	color: #F90;
	text-decoration: underline;
}

.menu-container {
	overflow: hidden;
	word-break: keep-all;
	white-space: nowrap;
	float:left;
}
.scroll-container{
	float: left;
	line-height: 75px;
	height: 75px;
	width: 14px;
	color: #82001F;
	background: #263849 url(../../images/topbg.png)  repeat-x;
}
.scroll-left {
	float: left;
	line-height: 80px;
	height: 80px;
	width: 14px;
	text-decoration: none;
	color:#FFFFFF;
	/*background: url(../../images/btn_bg.gif) repeat;*/
}
a.scroll-left:hover{
	color:#FFFFFF;
}
.scroll-right {
	float: left;
	line-height: 80px;
	height: 80px;
	width: 14px;
	text-decoration: none;
	color:#FFFFFF;
	/*background: url(../../images/btn_bg.gif) repeat;*/
}
a.scroll-right:hover{
	color:#FFFFFF;
}
#id_loadMaskContainer {
	width: 100%;
	height: 100%;
	z-index: 2;
}

.menuInfo_value input,menuInfo_value textarea {
	width: 200px;
}

.adviseButton {
	border-right: #7b9ebd 1px solid;
	padding-right: 2px;
	border-top: #7b9ebd 1px solid;
	padding-left: 2px;
	font-size: 12px;
	FILTER: progid : DXImageTransform.Microsoft.Gradient ( GradientType = 0,
		StartColorStr = #ffffff, EndColorStr = #cecfde );
	border-left: #7b9ebd 1px solid;
	cursor: hand;
	color: black;
	padding-top: 2px;
	border-bottom: #7b9ebd 1px solid;
}
