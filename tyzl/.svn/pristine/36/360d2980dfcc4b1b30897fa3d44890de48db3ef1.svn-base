@charset "utf-8";
html, body{
    margin:0;padding:0;border:0;width:100%;height:100%;
    overflow:hidden;
}
/*撑满form文本框*/
.miniext-form-fit{
	width: 95%;
}
.miniext-form-fit2{
	width: 98%;
}
/*table样式*/
.miniext-form-table{
	table-layout:fixed;width:100%;height:100%;padding: 10px;
}
/*td宽度样式*/
.miniext-form-td{
	width: 60px;
}
/*toolbar样式*/
.miniext-toolbar-bottom{
	border-bottom:0;padding:0px;text-align: center;
}
.miniext-toolbar-top{
	padding:2px;border-bottom:0;
}
.miniext-toolbar-title{
	font-size: 14px;color: red;
}
/*高极查询样式*/
fieldset
{
    border:solid 1px #aaa;
}        
.hideFieldset
{
    border-left:0;
    border-right:0;
    border-bottom:0;
}
.hideFieldset .fieldset-body
{
    display:none;
}
