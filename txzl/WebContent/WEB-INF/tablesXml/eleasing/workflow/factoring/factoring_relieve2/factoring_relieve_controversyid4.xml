<?xml version="1.0" encoding="UTF-8"?>
<table-root xmlns="http://www.tracywindy.org/table"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
	<table>
		<data>
			<alldb>
				<table_sql> 
	        		<![CDATA[
	        		
		  			select
		  				fc.id										as fcid,     
		  				fc.contract_id						as contractid,                 
                        fc.application_number  		as applicationnumber,                   		
                        fc.create_date               		as createdate,                                						
                        fc.explaination              		as explaination
                        
                      from  factoring_controversy fc WHERE 1=1 
					 /~contractid: AND fc.contract_id = '{contractid}' ~/
				 	]]>
				</table_sql>
			</alldb>
			<dataSource>dataSource</dataSource>
			<show_sql>true</show_sql>
			<Data_Authority>default</Data_Authority>
		</data>
	</table>
</table-root>


















