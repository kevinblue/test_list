<%@ page language="java" contentType="text/html; charset=UTF-8"  pageEncoding="UTF-8" %>
<div id="id_form_entity" class="mini-window" modal="true" title="报表管理" style="width: 450px">
	<center>
		<form id="form_entity">
        	<table style="width:90%">
        		<tr>
        			<td style="display: none;">
        				<input name="id" id="id"  class="mini-textbox"/>
        				<input name="position" id="position" class="mini-textbox" />
						<input name="parent" id="parent" class="mini-textbox" />
        			</td>
        		</tr>
	            <tr>
	                    <td class="input_label_desc" style="text-align: center;">名称:</td>
	                <td>
	                    <input name="entityName" id="entityName" class="mini-textbox" required="true"  style="width:200px;"  /><span class="content_required">*</span>
	                </td>
	            </tr>
	           
	            <tr>
	                   <td class="input_label_desc" style="text-align: center;">类型:</td>
	                <td>
	                    <input name="entityType" id="entityType" readonly class="mini-textbox" style="width:200px;"/><span class="content_required">*</span>
	                </td>
	            </tr>
	            
	             <tr id="tableNameTr">
	                   <td class="input_label_desc" style="text-align: center;">表名:</td>
	                <td>
	                    <input name="tableName" id="tableName" class="mini-textbox" style="width:200px;"/><span class="content_required">*</span>
	                </td>
	            </tr>
	            
	            <tr id="tableDescTr">
	                   <td class="input_label_desc" style="text-align: center;">表注释:</td>
	                <td>
	                    <input name="tableDesc" id="tableDesc" class="mini-textbox" style="width:200px;"/><span class="content_required">*</span>
	                </td>
	            </tr>
	            
	            <tr id="sourcefoldernameTr">
	                   <td class="input_label_desc" style="text-align: center;">source folder:</td>
	                <td>
	                    <input name="sourcefoldername" id="sourcefoldername" class="mini-combobox" style="width:200px;"/><span class="content_required">*</span>
	                </td>
	            </tr>
           
            	<tr>
					<td class="input_label_desc">&nbsp;</td>
					<td class="input_value">
						<table style="width:60%;padding-top: 15px;">
							<tr>	
								<td style="width:40%">&nbsp;</td>								
								<td><a id="entity_saveButton" class='mini-button'><span>确定</span></a></td>
								<td><a id="entity_closeButton" class='mini-button'><span>取消</span></a></td>									
							</tr>
						</table>
					</td>
				</tr>
			</table>
    	</form>
	</center>
</div>