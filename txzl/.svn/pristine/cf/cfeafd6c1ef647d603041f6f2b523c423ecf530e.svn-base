<?xml version="1.0" encoding="UTF-8"?>
<!--
合同审批清单列表

-->
<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
     <alldb>
      <table_sql> 
	        <![CDATA[
		select
		ci.id, 
		ci.contract_id,
		ci.contract_number,
		ci.project_name,
		ci.cust_name,
		ci.card_no,
		ci.industry_type,
		ci.PROJMANAGENAME projmanage,
		ci.department,
		ci.contractstatus,
		a.lawnum  from vi_contract_info ci
		right join law_approval a on ci.id = a.CONTRACTINFO_ID
		where 1=1  and ci.contract_status =31 
		/~contract_id: AND CI.CONTRACT_ID like '%{contract_id}%' ~/
		/~contract_number: AND CI.CONTRACT_NUMBER like '%{contract_number}%' ~/
		/~project_name: AND CI.PROJECT_NAME like '%{project_name}%' ~/
		/~cust_name: AND CI.CUST_NAME like '%{cust_name}%' ~/
		/~card_no: AND CI.CARD_NO like '%{card_no}%' ~/
				 ]]> 
	  </table_sql>
	   </alldb>
      <dataSource>dataSource</dataSource>
      <show_sql>true</show_sql>
    </data>
  </table>
</table-root>