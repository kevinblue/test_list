<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<input name="contract_change_info.id" id="contract_change_info.id" type="hidden" value="${requestScope['contract_change_info.id']}" />
<div id="proj_confrontation_form" title="风险对抗措施">
<div class="mini-panel"  showCollapseButton="true" style="width:100%;">
	<table cellspacing="0" cellpadding="0" style="width:100%;border: 1px solid #99CCFF;" class="fillTable">
		<tr class="tr-odd">
	             <td class="td-content-title" width="160px">担保人：</td>
	             <td colspan="3" style="padding-top: 5px;padding-bottom: 5px;"><input style="width:74%;height: 80px;" label="担保人" name="accident_info.guarantee" value="${requestScope['accident_info.guarantee'] }" class="mini-textarea"  type="text" >  </td>
	       </tr>
		<tr class="tr-even">
	             <td class="td-content-title" >租赁物件：</td>
	             <td colspan="3" style="padding-top: 5px;padding-bottom: 5px;"><input style="width:74%;height: 80px;" label="租赁物件" name="accident_info.equipinfo" value="${requestScope['accident_info.equipinfo'] }" class="mini-textarea"  type="text" >  </td>
	       </tr>
		<tr class="tr-odd">
	             <td class="td-content-title" >抵押物：</td>
	             <td colspan="3" style="padding-top: 5px;padding-bottom: 5px;"><input style="width:74%;height: 80px;" label="抵押物" name="accident_info.dyinfo" value="${requestScope['accident_info.dyinfo'] }" class="mini-textarea"  type="text" >  </td>
	       </tr>
		<tr class="tr-even">
	             <td class="td-content-title" >其他：</td>
	             <td colspan="3" style="padding-top: 5px;padding-bottom: 5px;"><input style="width:74%;height: 80px;" label="其他" name="accident_info.otherinfo" value="${requestScope['accident_info.otherinfo'] }" class="mini-textarea"  type="text" >  </td>
	       </tr>
	</table>
</div>
</div>
<script type="text/javascript">
jQuery(function(){
	if('${param.isView}' == 'true' || isViewHistoryTask==true){miniui_ext.disableFormFields("proj_confrontation_form");};
});
</script>
