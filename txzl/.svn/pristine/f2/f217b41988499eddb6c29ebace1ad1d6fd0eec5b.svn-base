<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
					select tw.id twid,
						tw.week_id weekid,
			            tw.week_one weekone,
			            tw.week_two weektwo,
			            tw.week_three weekthree,
			            tw.week_four weekfour,
			            tw.week_five weekfive,
			            tw.week_six  weeksix,
                  	    tw.week_seven weekseven
			            from t_work_week_report_detail tw
			            where 1=1 and tw.workweek_id = '{workid}'
			 ]]> 
	  </table_sql>
	  </alldb>
      <dataSource>dataSource</dataSource>
      <show_sql>true</show_sql>
      <Data_Authority>default</Data_Authority>
    </data>
  </table>
</table-root>