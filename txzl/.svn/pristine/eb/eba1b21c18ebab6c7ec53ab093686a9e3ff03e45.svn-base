<?xml version="1.0" encoding="UTF-8"?>
<!-- 选择客户的银行 -->
<table-root xmlns="http://www.tracywindy.org/table"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
	<table>
		<data>
			<alldb>
				<table_sql> 
	        <![CDATA[
	          select 
	          bank_name                      clientbank,
		       cust_account.deposit_bank_type depositbanktype,
		       account                        clientaccount,
		       acc_number                     clientaccnumber,
		       bank_no                        clientbankno,
		       cust_account.union_bank_no     unionclientbankno,
		       cust_account.deposit_province  depositprovince,
		       cust_account.deposit_city      depositcity
			  from cust_account
			  left outer join cust_info
		  	  on cust_info.id = cust_account.cust_id
				 where 1 = 1
               
             
               /~custid:and cust_account.cust_id='{custid}'~/
               /~custname:and cust_info.cust_name='{custname}'~/
			 ]]>

				</table_sql>
			</alldb>
			<dataSource>dataSource</dataSource>
			<show_sql>true</show_sql>
		</data>
	</table>
</table-root>