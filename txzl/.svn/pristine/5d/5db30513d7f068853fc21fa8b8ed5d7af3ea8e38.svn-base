    <alldb>
      <table_sql> 
	        <![CDATA[   
	            select * from (
			  	     select  BASE_FILE basefile,OPERATOR_TYPE operatortype,create_date createdate,T_USERS.realname_ creator 
                     from base_file_recorder
                     left outer join T_USERS
                     on T_USERS.id_=base_file_recorder.creator_)rs
                     where 1=1
                 /~id:and rs.basefile='{id}'~/
                 <EXTRACTEND>order by createdate desc<EXTRACTEND>
				 ]]> 
	  </table_sql>
