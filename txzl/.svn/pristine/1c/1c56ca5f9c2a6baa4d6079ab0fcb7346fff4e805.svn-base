function dateSformat(date){
	return date.replace('-','年').replace('-','月')+'日';
} 

function getQuarter(time){
	if(time!=null && time!=""){
		time = mini.formatDate(time,"MM");
		return Math.floor((time%3==0)?(time/3):(time/3+1));
	}else{
		return "";
