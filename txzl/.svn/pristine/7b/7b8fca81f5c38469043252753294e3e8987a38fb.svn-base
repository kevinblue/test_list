<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
				SELECT  t.lease_person as leaseperson,
        t.lease_linkman as leaselinkman,
        t.lease_addr as leaseaddr,
        t.lease_tel as leasetel,
        t.lease_fax as leasefax,
        t.lease_postcode as leasepostcode,
				t.ID               as oid,
				t.OWN_NAME         as raw_oid,
				t.org_code         as orgcode
				FROM own_info t 
			 ]]> 
	  </table_sql>
	  </alldb>
      <dataSource>dataSource</dataSource>
    </data>
  </table>
</table-root>