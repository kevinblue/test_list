<?xml version="1.0" encoding="UTF-8"?>
<!-- 选择客户的银行-->
<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
	        	
	             SELECT CI.ID AS VALUE, CI.CUST_NAME AS NAME,
	              t.name_ as customertypename,
	              t.code_ as customertype
                  FROM
                  (
                  SELECT CUST_ID FROM CONTRACT_INFO   where id = '{contractid}'
                  UNION
                  SELECT seller FROM CONTRACT_SUPPLIER_INFO  where contract_id='{contractid}'
                  )C
                  LEFT JOIN  CUST_INFO CI ON C.CUST_ID = CI.ID
                  LEFT OUTER JOIN
                  (SELECT wm_concat(cust_type) custtype,cust_id from  CUST_TYPE_INFO 
                  group by cust_id )CTI ON CI.ID = CTI.CUST_ID
                  left join t_dicts_datas t on ci.cust_omer_type = t.code_
                 WHERE 1=1 and invalid_='否'
               /~name:and CI.CUST_NAME like '%{name}%'~/
               /~custtype:and CTI.CUSTTYPE like '%{custype}%'~/
               /~customer:and ci.cust_name ='{customer}'~/
              	
			 ]]> 
			 
	  </table_sql>
	  </alldb>
	  <sqlserver>
	     <table_sql>
	       <![CDATA[
	             SELECT CI.ID AS VALUE, CI.CUST_NAME AS NAME
                  FROM CUST_INFO CI
                  LEFT OUTER JOIN
                  ( SELECT  [custtype]=stuff((select ','+isnull([cust_type],'') 
                     from CUST_TYPE_INFO t where 
                    t.CUST_ID=CUST_TYPE_INFO.CUST_ID order by t.CUST_ID desc  for xml path('')), 1, 1, ''),
                    cust_id from  CUST_TYPE_INFO 
                  group by cust_id )CTI ON CI.ID = CTI.CUST_ID
                 WHERE 1=1 and invalid_='否'
               /~name:and CI.CUST_NAME like '%{name}%'~/
               /~custtype:and CTI.CUSTTYPE like '%{custype}%'~/
			 ]]> 
	     </table_sql>
	  </sqlserver>
      <dataSource>dataSource</dataSource>
      <show_sql>true</show_sql>
    </data>
  </table>
</table-root>