<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
	        select ttt.* from (
			  SELECT BM.ID AS ID,
	          BM.MSG_TYPE AS MSGTYPE,
	          BM.MSG_TITLE AS MSGTITLE,
	          BM.SUBJECT_ AS MSGTEXT,
	          BM.TO_USER_TYPE AS USERTYPE,
	          BM.FROMUSER AS FROMUSER,
	          BM.SEND_DATE AS SENDDATE,
	          (SELECT WMSYS.WM_CONCAT(BMTG.GOUP_ID) USERS FROM BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_user'
	          GROUP BY 'msggroup_user') USERS,
	          (SELECT WMSYS.WM_CONCAT(BMTG.GOUP_ID) GROUPSS FROM BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_group'
	          GROUP BY 'msggroup_group') GROUPSS,
	          (SELECT WMSYS.WM_CONCAT(BMTG.GOUP_ID) DEPTS FROM BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_dept'
	          GROUP BY 'msggroup_dept') DEPTS,
	          (SELECT WMSYS.WM_CONCAT(BMTG.GOUP_ID) ROLESS FROM BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_roles'
	          GROUP BY 'msggroup_roles') ROLESS,
	          (SELECT WMSYS.WM_CONCAT(BMTG.GOUP_ID) DISTRICTS FROM BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_district'
	          GROUP BY 'msggroup_district') DISTRICTS,
	          (SELECT WMSYS.WM_CONCAT(TU.REALNAME_) USER_NAME FROM BASE_MESSAGE_TO_USER BMTU
		      LEFT JOIN T_USERS TU ON TU.ID_ = BMTU.READ_USER WHERE BMTU.MSG_ID = BM.ID
	          GROUP BY BMTU.MSG_ID) REALNAME,
	          (SELECT WMSYS.WM_CONCAT(TU.ID_) USER_NAME FROM BASE_MESSAGE_TO_USER BMTU
		      LEFT JOIN T_USERS TU ON TU.ID_ = BMTU.READ_USER WHERE BMTU.MSG_ID = BM.ID
	          GROUP BY BMTU.MSG_ID) READUSER
	          FROM BASE_MESSAGE BM
	          WHERE 1=1
		      /~MSG_TYPE: AND BM.MSG_TYPE = '{MSG_TYPE}'~/
		      /~FROM_USER: AND BM.FROMUSER = '{FROM_USER}'~/
		      )ttt
		      where 1=1
		      /~realname: AND REALNAME like '%{realname}%'~/
		      
			 ]]> 
	  </table_sql>
	  </alldb>
	  <sqlserver>
      <table_sql> 
	        <![CDATA[
	        select ttt.* from (
			  SELECT BM.ID AS ID,
	          BM.MSG_TYPE AS MSGTYPE,
	          BM.MSG_TITLE AS MSGTITLE,
	          BM.SUBJECT_ AS MSGTEXT,
	          BM.TO_USER_TYPE AS USERTYPE,
	          BM.FROMUSER AS FROMUSER,
	          BM.SEND_DATE AS SENDDATE,
	          [USERS]=stuff((select ','+[GOUP_ID] from BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_user' for xml path('')), 1, 1, ''),
	          [GROUPSS]=stuff((select ','+[GOUP_ID] from BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_group' for xml path('')), 1, 1, ''),
	          [DEPTS]=stuff((select ','+[GOUP_ID] from BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_dept' for xml path('')), 1, 1, ''),
	          [ROLESS]=stuff((select ','+[GOUP_ID] from BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_roles' for xml path('')), 1, 1, ''),
	          [DISTRICTS]=stuff((select ','+[GOUP_ID] from BASE_MESSAGE_TO_GROUP BMTG 
	          WHERE BMTG.MSG_ID = BM.ID AND BMTG.MSG_GROUP = 'msggroup_district' for xml path('')), 1, 1, ''),
	          [REALNAME]=stuff((select ','+[REALNAME_] FROM BASE_MESSAGE_TO_USER BMTU
		      LEFT JOIN T_USERS TU ON TU.ID_ = BMTU.READ_USER WHERE BMTU.MSG_ID = BM.ID for xml path('')), 1, 1, ''),
	         [READUSER]=stuff((select ','+[ID_] FROM(select tu.id_,BMTU.MSG_ID from BASE_MESSAGE_TO_USER BMTU
		      LEFT JOIN T_USERS TU ON TU.ID_ = BMTU.READ_USER )  BMTU WHERE BMTU.MSG_ID = BM.ID for xml path('')), 1, 1, '')
	          FROM BASE_MESSAGE BM
	          WHERE 1=1
		      /~MSG_TYPE: AND BM.MSG_TYPE = '{MSG_TYPE}'~/
		      /~FROM_USER: AND BM.FROMUSER = '{FROM_USER}'~/
		      )ttt
		      where 1=1
		      /~realname: AND REALNAME like '%{realname}%'~/
		      <EXTRACTEND>order by SENDDATE desc<EXTRACTEND>
			 ]]> 
	  </table_sql>
	  </sqlserver>
