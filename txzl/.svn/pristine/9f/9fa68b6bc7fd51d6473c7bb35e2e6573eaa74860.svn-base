<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
		         select  ENTITYCOLUMN.ID_ id,
				ENTITYCOLUMN.FIELD_NAME FIELDNAME,
				ENTITYCOLUMN.TABLE_IS_INDEX TABLEISINDEX,
				ENTITYCOLUMN.TABLE_INDEX_NAME TABLEINDEXNAME,
				ENTITYCOLUMN.FIELD_TYPE FIELDTYPE,
				ENTITYCOLUMN.FIELD_TYPE_FULL_NAME FIELDTYPEFULLNAME,
				ENTITYCOLUMN.TABLE_COLUMN_NAME TABLECOLUMNNAME,
				ENTITYCOLUMN.TABLE_COLUMN_TYPE TABLECOLUMNTYPE,
				ENTITYCOLUMN.TABLE_COLUMN_LENGTH TABLECOLUMNLENGTH,
				ENTITYCOLUMN.PID_ ENTITY,
				ENTITYCOLUMN.TABLE_IS_NOTNULL TABLEISNOTNULL,
				ENTITYCOLUMN.TABLE_IS_UNIQUE TABLEISUNIQUE,
				ENTITYCOLUMN.POSITION_ TABLECOLUMNPOSITION,
				ENTITYCOLUMN.TABLE_COLUMN_DESC TABLECOLUMNDESC,
				ENTITYCOLUMN.TABLE_COLUMN_DEFINITION TABLECOLUMNDEFINITION,
				ENTITYCOLUMN.TABLE_COLUMN_PRECISION TABLECOLUMNPRECISION,
				ENTITYCOLUMN.COLUMN_ORDER_BY COLUMNORDERBY,
				ENTITYCOLUMN.TABLE_FETCH_TYPE FETCHTYPE,
				ENTITYCOLUMN.TABLE_COLUMN_SCALE TABLECOLUMNSCALE
				from  T_FREEMAKER_ENTITY_column ENTITYCOLUMN
				WHERE 1=1 
				 /~entityId: and ENTITYCOLUMN.PID_ = '{entityId}'~/
				 <EXTRACTEND>order by TABLECOLUMNPOSITION ASC<EXTRACTEND>
			 ]]> 
	  </table_sql>
	  </alldb>
      <dataSource>dataSource</dataSource>
      <show_sql>false</show_sql>
    </data>
  </table>
</table-root>