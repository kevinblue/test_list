<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[
			select id,
               model_name     modelname,
               file_name      filename,
               file_key       filekey,
               tu.realname_   creator,
               create_date    createdate,
               invalid_       invalid
               from base_file
               left outer join t_users tu on tu.id_=base_file.creator_
                  where 1=1 
              /~modelname: and  model_name ='{modelname}' ~/
                and  file_key  in 
                ( select id from CONTENT_MATERIAL where 1=1 )
                     
                  <EXTRACTEND>     order by createdate desc ,invalid <EXTRACTEND>    
			 ]]> 
	  </table_sql>
	  </alldb>
           <dataSource>dataSource</dataSource>
      <show_sql>true</show_sql>
     </data>
  </table>
</table-root>