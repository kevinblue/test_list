<?xml version="1.0" encoding="UTF-8"?>

<table-root 
  xmlns="http://www.tracywindy.org/table" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.tracywindy.org/table http://www.tracywindy.org/table/table-1.0.xsd">
  <table>
    <data>
    <alldb>
      <table_sql> 
	        <![CDATA[   
				select t.id_ id,
				       t.must_update mustupdate,
				       t.version_id versionid,
				        t.version_code versioncode,
                        t.version_name versionname,
				       t.up_date updates,
				       t.image_path imagepath,
				       t.phone_type phonetype,
				       t.base_file_id basefileid,
				       t.is_new isnew,
				       t.creator_,
				       t.create_date,
				       t.modificator_,
				       t.modify_date
				from t_app_version t
                order by t.up_date desc
                       
				 ]]> 
	  </table_sql>
	  </alldb>
      <dataSource>dataSource</dataSource>
      <show_sql>true</show_sql>
    </data>
  </table>
</table-root>